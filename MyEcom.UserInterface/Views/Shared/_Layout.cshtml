<script>
        $("input").keyup(function () {
            if ($("input").val() != "") {

                $.ajax({
                    type: "Get",
                    url: "/Home/Search",
                    data: { "key": $("input").val() }

                }).done(function (data) {

                    $("#list").html(data);
                    //$("#list").append('<li><a href="/Home/PreviewRoom" ><span class="title">"' + data[0].HotelName + '"</span ></a ></li >');
                    $.each(data, function (index) {
                        $("#list").append(data[index])
                        $("#list").append('<li><a href="/Home/Hotel/' + data[index].Id + '" ><span class="title" style="font-size:18px;color:#2C2E3E;">' + data[index].Name + '</span ></a ></li >');
                    });
                });

            } else {
                $("#list").html(" ");

            }
        });


</script>